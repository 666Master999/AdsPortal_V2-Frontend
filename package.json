{
  "name": "adsportal-v2-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx,.cjs,.mjs --fix",
    "format": "prettier --write \"src/**/*.{vue,js,jsx,ts,tsx,css,scss,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "pinia": "^2.1.7",
    "vue": "^3.5.25",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^10.0.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "^3.1.1",
    "vue-tsc": "^3.2.5"
  }
}
